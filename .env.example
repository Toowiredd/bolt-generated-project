NODE_ENV=development
PORT=3000
JWT_SECRET=your-secret-key-here
VALTOWN_API_KEY=your-valtown-api-key
VALTOWN_ENDPOINT=https://api.val.town/v1

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
